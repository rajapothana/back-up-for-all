    CtrlbttnCorrRequired.disabled = true;
}

function ChgCorrectionRequiredAdmin() {
    var ChkCorrRequired = document.getElementById('ctl00_ContentPlaceHolder1_chkCorrRequired');
    var CtrlbttnCorrRequired = document.getElementById('ctl00_ContentPlaceHolder1_bttnCorrRequired');

    if (ChkCorrRequired.checked == true) {
        CtrlbttnCorrRequired.disabled = false;
        return;
    }
    if (ChkCorrRequired.checked == false) {
        CtrlbttnCorrRequired.disabled = true;
        return;
    }
}

function ChgCorrectionRequired() {
    var ChkCorrRequired = document.getElementById('ctl00_ContentPlaceHolder1_chkCorrRequired');
    var ChkNotRequired = document.getElementById('ctl00_ContentPlaceHolder1_chkCorrNotRequired');
    var CtrlbttnCorrRequired = document.getElementById('ctl00_ContentPlaceHolder1_bttnCorrRequired');
    var CtrlbttnExit = document.getElementById('ctl00_ContentPlaceHolder1_bttnLock');
    ChkNotRequired.checked = false;
    if (ChkCorrRequired.checked == true) {
        CtrlbttnCorrRequired.disabled = false;
        CtrlbttnExit.disabled = true;
        return;
    }
    if (ChkCorrRequired.checked == false) {
        CtrlbttnCorrRequired.disabled = true;
        CtrlbttnExit.disabled = true;
        return;
    }
}


function ChgCorrectionNotRequired() {
    var ChkCorrRequired = document.getElementById('ctl00_ContentPlaceHolder1_chkCorrRequired');
    var ChkNotRequired = document.getElementById('ctl00_ContentPlaceHolder1_chkCorrNotRequired');
    var CtrlbttnCorrRequired = document.getElementById('ctl00_ContentPlaceHolder1_bttnCorrRequired');
    var CtrlbttnExit = document.getElementById('ctl00_ContentPlaceHolder1_bttnLock');
    ChkCorrRequired.checked = false;
    if (ChkNotRequired.checked == true) {
        CtrlbttnCorrRequired.disabled = true;
        CtrlbttnExit.disabled = false;
        return;
    }
    if (ChkNotRequired.checked == false) {
        CtrlbttnCorrRequired.disabled = true;
        CtrlbttnExit.disabled = true;
        return;
    }
}

function EncData(salt) {

    Page_ClientValidate();
    if (Page_IsValid) {
    }
    else {

        return false;
    }

    if (document.getElementById('ctl00_ContentPlaceHolder1_ddlfeemode').value == "0" || document.getElementById('ctl00_ContentPlaceHolder1_txtappno').value == "" || document.getElementById('ctl00_ContentPlaceHolder1_dob').value == "") {
        alert('All fields are mandatory?');
        return false;
    }
    document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value = "";
    document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value = document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value + document.getElementById('ctl00_ContentPlaceHolder1_ddlfeemode').value.toUpperCase();
    document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value = document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value + document.getElementById('ctl00_ContentPlaceHolder1_txtappno').value.toUpperCase();
    document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value = document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value + document.getElementById('ctl00_ContentPlaceHolder1_dob').value.toUpperCase();
    document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value = (hex_md5(document.getElementById('ctl00_ContentPlaceHolder1_hiddvalue').value + salt)).toUpperCase();
    return (true);
}

function EncDataSynd(salt) {
    if (document.getElementById('ctl00_ContentPlaceHolder1_txtappno').value == "" || document.getElementById('ctl00_ContentPlaceHolder1_txtname').value == "" || document.getElementById('ctl00_ContentPlaceHolder1_txtfee').value == "") {
        alert('All fields are mandatory?');
        return false;
    }
    document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value = "";
    document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value = document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value + document.getElementById('ctl00_ContentPlaceHolder1_txtappno').value.toUpperCase();
    document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value = document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value + document.getElementById('ctl00_ContentPlaceHolder1_txtname').value.toUpperCase();
    document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value = document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value + document.getElementById('ctl00_ContentPlaceHolder1_txtfee').value.toUpperCase();

    document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value = (hex_md5(document.getElementById('ctl00_ContentPlaceHolder1_txthidd').value + salt)).toUpperCase();
    return (true);
}

function IsDobBlank(sender, args) {
    var ctrDay = document.getElementById('ctl00_ContentPlaceHolder1_ddlday');
    var ctrMonth = document.getElementById('ctl00_ContentPlaceHolder1_ddlmonth');
    var ctrYear = document.getElementById('ctl00_ContentPlaceHolder1_ddlyear');
    if (ctrDay.value == "0" || ctrMonth.value == "0" || ctrYear.value == "0") {
        args.IsValid = false;
        ctrDay.focus();
        return;
    }
}

function isDobDateValid(sender, args) {
    var ctrDay = document.getElementById('ctl00_ContentPlaceHolder1_ddlday');
    var ctrMonth = document.getElementById('ctl00_ContentPlaceHolder1_ddlmonth');
    var ctrYear = document.getElementById('ctl00_ContentPlaceHolder1_ddlyear');
    if (ctrDay.value != "0" && ctrMonth.value != "0" && ctrYear.value != "0") {

        var a = ctrDay.value + "/" + ctrMonth.value + "/" + ctrYear.value;
        if (a.length < 10 || a.length > 10) {
            args.IsValid = false;
            ctrDay.value = "0";
            return;
        }
        var datePat = /^(\d{2})(\/)(\d{2})\2(\d{4})$/;
        var matchArray = a.match(datePat); // is the format ok?
        if (matchArray == null) {
            args.IsValid = false;
            ctrDay.value = "0";
            return;
        }
        day = ctrDay.value; // parse date into variables
        month = ctrMonth.value;
        year = ctrYear.value;
        if (month < 1 || month > 12) { // check month range
            args.IsValid = false;
            ctrDay.value = "0";
            return;
        }
        if (day < 1 || day > 31) {
            args.IsValid = false;
            ctrDay.value = "0";
            return;
        }
        if ((month == 4 || month == 6 || month == 9 || month == 11) && day == 31) {
            args.IsValid = false;
            ctrDay.value = "0";
            return;
        }
        if (month == 2) { // check for february 29th
            var isleap = (year % 4 == 0 && (year % 100 != 0 || year % 400 == 0));
            if (day > 29 || (day == 29 && !isleap)) {
                args.IsValid = false;
                ctrDay.value = "0";
                return;
            }
        }
        args.IsValid = true;  // date is valid
    }
}

function isvalidBOB(which) {
    var ddlcat = document.getElementById('ctl00_ContentPlaceHolder1_ddlCat');
    var ddlph = document.getElementById('ctl00_ContentPlaceHolder1_ddlPh');
    var ctrDay = document.getElementById('ctl00_ContentPlaceHolder1_ddlday');
    var ctrMonth = document.getElementById('ctl00_ContentPlaceHolder1_ddlmonth');
    var ctrYear = document.getElementById('ctl00_ContentPlaceHolder1_ddlyear');
    var ctrstelig = document.getElementById('ctl00_ContentPlaceHolder1_ddcodeelig1');
    if (ctrDay.value != "0" && ctrMonth.value != "0" && ctrYear.value != "0" && ddlcat.value != "0" && ddlph.value != "0" && ctrstelig.value != "26" && ctrstelig.value != "20") {
        var yy = ctrYear.value;
        var dd = ctrDay.value;
        var mm = ctrMonth.value;
        if (yy > 2003) {
            alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth");
            ctrDay.value = "0";
            ctrMonth.value = "0";
            ctrYear.value = "0";
            ctrDay.focus();

        }
        //--------- DOB for SC/ST candidates ---------
        if ((ddlcat.value == "2" || ddlcat.value == "3")) {
            if (yy <= 1987) {
                if (yy == 1987 & mm <= 9) {
                    ctrDay.value = "0";
                    ctrMonth.value = "0";
                    ctrYear.value = "0";
                    ctrDay.focus();
                    alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth.For more information click Help");


                } else if (yy < 1987) {
                    ctrDay.value = "0";
                    ctrMonth.value = "0";
                    ctrYear.value = "0";
                    ctrDay.focus();
                    alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth.For more information click Help");


                }
            }
            //--------- DOB for General/OBC candidates for PH -----
        } else if ((ddlcat.value == "1" || ddlcat.value == "4")) {
            if (ddlph.value == "1") {
                if (yy <= 1987) {
                    if (yy == 1987 & mm <= 9) {
                        ctrDay.value = "0";
                        ctrMonth.value = "0";
                        ctrYear.value = "0";
                        ctrDay.focus();
                        alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth.For more information click Help");


                    } else if (yy < 1987) {

                        alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth.For more information click Help");

                        ctrDay.value = "0";
                        ctrMonth.value = "0";
                        ctrYear.value = "0";
                        ctrDay.focus();

                    }
                }
                //--------- DOB for General/OBC candidates For Non PH -----
            } else if (ddlph.value == "2") {
                if (yy <= 1992) {
                    if (yy == 1992 & mm <= 9) {

                        alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth.For more information click Help");

                        ctrDay.value = "0";
                        ctrMonth.value = "0";
                        ctrYear.value = "0";
                        ctrDay.focus();

                    } else if (yy < 1992) {
                        alert("You are not eligible for JEE(Main) 2019 on the basis of Date Of Birth.For more information click Help");

                        ctrDay.value = "0";
                        ctrMonth.value = "0";
                        ctrYear.value = "0";
                        ctrDay.focus();

                    }
                }
            }
        }

    }
}



function CustomValIsOnlyChild(sender, args) {
    var ctrlGen = document.getElementById('ctl00_ContentPlaceHolder1_ddlGender');
    var ctrlCh1 = document.getElementById('ctl00_ContentPlaceHolder1_ddlOnlyChild');
    if (ctrlGen.value == "2" && args.Value == "0") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }
}

function ChgSex() {
    var ctrlGen = document.getElementById('ctl00_ContentPlaceHolder1_ddlGender');
    var ctrlCh1 = document.getElementById('ctl00_ContentPlaceHolder1_ddlOnlyChild');
    if (ctrlGen.value == "2")
        ctrlCh1.disabled = false
    else if (ctrlGen.value == "1" || ctrlGen.value == "3" || ctrlGen.value == "0") {
        ctrlCh1.disabled = true
        ctrlCh1.value = "0"
    }
    return false;
}

function CustomValIsBlind(sender, args) {
    var ctrlDdlBlind = document.getElementById('ctl00_ContentPlaceHolder1_ddlBlind');
    var ctrlDdlPh = document.getElementById('ctl00_ContentPlaceHolder1_ddlPh');
    if (ctrlDdlPh.value != "0" && args.Value == "0") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}

function CustomValPhPer(sender, args) {
    var ctrltxtPhPer = document.getElementById('ctl00_ContentPlaceHolder1_txtPhPer');
    var ctrlDdlPh = document.getElementById('ctl00_ContentPlaceHolder1_ddlPh');
    if (ctrlDdlPh.value == "1" && ctrltxtPhPer.value == "") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}


function ChgBoard() {
    var ctrlDdlBoard = document.getElementById('ctl00_ContentPlaceHolder1_ddschboard');
    document.getElementById('ctl00_ContentPlaceHolder1_txtRollNo').value = "";
    if (ctrlDdlBoard.value == "99") {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlOtherBoard').style.display = 'block';
    }
    else {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlOtherBoard').style.display = 'none';
    }

    return false;
}


function CustomValIsAmanuenses(sender, args) {
    var ctrlDdlPH = document.getElementById('ctl00_ContentPlaceHolder1_ddlPh');
    var ctrlDdlAmanuenses = document.getElementById('ctl00_ContentPlaceHolder1_ddlneedamanuenses');
    var a = parseInt(document.getElementById('ctl00_ContentPlaceHolder1_txtPhPer').value);
    if (ctrlDdlPH.value == "1" && ctrlDdlAmanuenses.value == "0" && a > 39) {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}
function ChgIsblind() {
    var ctrlDdlBlind = document.getElementById('ctl00_ContentPlaceHolder1_ddlBlind');
    var ctrlDdlneedamanuenses = document.getElementById('ctl00_ContentPlaceHolder1_ddlneedamanuenses');
    if (ctrlDdlBlind.value == "2") {
        ctrlDdlneedamanuenses.value = "2"
        ctrlDdlneedamanuenses.disabled = true
    }
    else if (ctrlDdlBlind.value == "1" || ctrlDdlBlind.value == "0") {
        ctrlDdlneedamanuenses.value = "0"
        ctrlDdlneedamanuenses.disabled = false
    }
    return false;
}

function ChgXPassYear() {
    var ctrl = document.getElementById('ctl00_ContentPlaceHolder1_ddyearpassXII');
    var ctr2 = document.getElementById('ctl00_ContentPlaceHolder1_ddlYearPassX');
    var ctr3 = document.getElementById('ctl00_ContentPlaceHolder1_ddlyear');
    if (ctrl.value != "0" && ctr2.value != "0") {
        if (parseInt(ctrl.value) - parseInt(ctr2.value) < 2) {
            alert("Minimum difference between 'Year of passing class 10th or its equivalent' and 'Year of passing or appearing class 12th or its equivalent' should be 2 year");
            ctr2.value = "0";
            ctrl.value = "0";
            ctr2.focus();
        }
    }
    if (ctr2.value != "0" && ctr3.value != "0") {
        if (parseInt(ctr2.value) - parseInt(ctr3.value) < 11) {
            alert("Minimum difference between 'Year of passing class 10th/equivalent' and 'Date of Birth' should be 11 year.");
            ctr2.value = "0";
            ctr2.focus();
        }
    }

    return true;

}

function CustomValIsPercentage(sender, args) {
    var ctrlDdlXIIPassYear = document.getElementById('ctl00_ContentPlaceHolder1_ddyearpassXII');
    var ctrlXIITxt = document.getElementById('ctl00_ContentPlaceHolder1_txtperXII');
    if (ctrlDdlXIIPassYear.value != "2019" && ctrlDdlXIIPassYear.value != "0" && args.Value == "") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}

function CustomValIsimp(sender, args) {
    var ctrlDdlXIIPassYear = document.getElementById('ctl00_ContentPlaceHolder1_ddyearpassXII');
    var ctrlXIITxt = document.getElementById('ctl00_ContentPlaceHolder1_ddlImproveMent');
    if (ctrlDdlXIIPassYear.value != "2019" && ctrlDdlXIIPassYear.value != "0" && args.Value == "0") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}



function CustomValMobileNo(sender, args) {
    var ctrlRoll = document.getElementById('ctl00_ContentPlaceHolder1_txtmobileno');
    if (ctrlRoll.value != "") {
        var str = ctrlRoll.value;
        if (str.charAt(0) == "9" || str.charAt(0) == "8" || str.charAt(0) == "7" || str.charAt(0) == "6") {
            args.IsValid = true;
        }
        else {
            args.IsValid = false;
        }

    }
}


function CustomValRoll(sender, args) {
    var ctrlDdlXIIPassYear = document.getElementById('ctl00_ContentPlaceHolder1_ddyearpassXII');
    var ctrlRoll = document.getElementById('ctl00_ContentPlaceHolder1_txtRollNo');
    if (ctrlDdlXIIPassYear.value != "2019" && ctrlDdlXIIPassYear.value != "0" && args.Value != "") {
        var flag = false;
        var indx = "0123456789";
        var str = ctrlRoll.value;
        if (str.length > 0) {
            for (i = 0; i < str.length; i++) {
                if (indx.indexOf(str.charAt(i)) >= 0) {
                    var flag = true;
                }
            }
        }
    }

    if (flag == false) {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }
}

