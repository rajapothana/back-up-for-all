function CustomValAddState(sender, args) {
    var ctrlDdlCountry = document.getElementById('ctl00_ContentPlaceHolder1_ddlCountry');
    var ctrlDdlState = document.getElementById('ctl00_ContentPlaceHolder1_ddlState');
    if (ctrlDdlCountry.value == "IN" && ctrlDdlState.Value == "0") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}

function CustomValAddDistrict(sender, args) {
    var ctrlDdlCountry = document.getElementById('ctl00_ContentPlaceHolder1_ddlCountry');
    var ctrlddlDistrict = document.getElementById('ctl00_ContentPlaceHolder1_ddlDistrict');
    if (ctrlDdlCountry.value == "IN" && ctrlddlDistrict.Value == "0") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}

function CustomValIsAadhar(sender, args) {
    var ctrlDdlAadhar = document.getElementById('ctl00_ContentPlaceHolder1_ddlNationality');
    var ctrtxtAadhaarNum = document.getElementById('ctl00_ContentPlaceHolder1_txtAadhaarNum');
    if (ctrlDdlAadhar.value == "1" && ctrlDdlAadhar.value != "0" && args.Value == "") {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }

}

function PnlAadhaarConsent() {
    var ctrlddlNat = document.getElementById('ctl00_ContentPlaceHolder1_ddlNationality');
    var ctrlBttnSubmit = document.getElementById('ctl00_ContentPlaceHolder1_submitbutton');
    var ctrlpnlAadhaarConsent = document.getElementById('ctl00_ContentPlaceHolder1_pnlAadhaarConsent');
    var ctrltrAadhaarNo = document.getElementById('ctl00_ContentPlaceHolder1_trAadhaarNo');
    if (ctrlddlNat.value == "1") {
        ctrlpnlAadhaarConsent.style.display = 'block';
        ctrltrAadhaarNo.style.display = '';
        ctrlBttnSubmit.disabled = true;
    }
    else {
        ctrlpnlAadhaarConsent.style.display = 'none';
        ctrltrAadhaarNo.style.display = 'none';
        ctrlBttnSubmit.disabled = false;
    }

}


function OnChagNationality() {
    var ctrlBttnSubmit = document.getElementById('ctl00_ContentPlaceHolder1_ddcodeelig1');
    ctrlDdlCountry.value = '0';
   }



function ChgAadhaarConsent() {

    var ChkIAgree = document.getElementById('ctl00_ContentPlaceHolder1_chkagreement');
    var ctrlBttnSubmit = document.getElementById('ctl00_ContentPlaceHolder1_submitbutton');
    if (ChkIAgree.checked == true) {
        ctrlBttnSubmit.disabled = false;
        return;
    }
    if (ChkIAgree.checked == false) {
        ctrlBttnSubmit.disabled = true;
        return;
    }
}


function PnlPayMode() {
    document.getElementById('ctl00_ContentPlaceHolder1_Pnlmain').style.display = 'block';
    return false;
}

function DisableSubmitOnAadhaarConsent() {
  
    var ctrlBttnSubmit = document.getElementById('ctl00_ContentPlaceHolder1_submitbutton');
    var ctrlddlVerificationType = document.getElementById('ctl00_ContentPlaceHolder1_ddlVerificationType');
    var txtVerificationId = document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId');
    txtVerificationId.value = "";
    document.getElementById('ctl00_ContentPlaceHolder1_lblVerificationId').innerHTML = ctrlddlVerificationType.options[ctrlddlVerificationType.selectedIndex].text;

   if (ctrlddlVerificationType.value == "01") {
       ctrlBttnSubmit.disabled = false;
    }
    else {
      
        ctrlBttnSubmit.disabled = false;
    }

}

function onChangeVerificationType() {
    var txtVerificationId = document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId');
    var ctrlddlVerificationType = document.getElementById('ctl00_ContentPlaceHolder1_ddlVerificationType');
    txtVerificationId.value = "";
    document.getElementById('ctl00_ContentPlaceHolder1_lblVerificationIdCaption').innerHTML = ctrlddlVerificationType.options[ctrlddlVerificationType.selectedIndex].text;
}

function onchangeddlCat() {
    if ($("#ctl00_ContentPlaceHolder1_ddlCat").val() != '1') {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlCastNm').style.display = 'block';
        $("#ctl00_ContentPlaceHolder1_txtCastNm").val("");
    }
    else {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlCastNm').style.display = 'none';
        $("#ctl00_ContentPlaceHolder1_txtCastNm").val('NA');
    }

}


function onchangeddlPwD() {
    if ($("#ctl00_ContentPlaceHolder1_ddlPh").val() == '1' ) {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlisPh').style.display = 'block';
        $("#ctl00_ContentPlaceHolder1_ddlneedamanuenses").val('0');
        $("#ctl00_ContentPlaceHolder1_txtPhPer").val('');
    }
    else {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlisPh').style.display = 'none';
        $("#ctl00_ContentPlaceHolder1_ddlneedamanuenses").val('2');
        $("#ctl00_ContentPlaceHolder1_txtPhPer").val('10');
        
    }

}


function onchangeddlGender() {
    if ($("#ctl00_ContentPlaceHolder1_ddlGender").val() == '1' || $("#ctl00_ContentPlaceHolder1_ddlGender").val() == '3' || $("#ctl00_ContentPlaceHolder1_ddlGender").val() == '0') {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlOnlyGirlChild').style.display = 'none';
        $("#ctl00_ContentPlaceHolder1_ddlOnlyChild").val('2');
    }
    else if ($("#ctl00_ContentPlaceHolder1_ddlGender").val() == '2'){
        document.getElementById('ctl00_ContentPlaceHolder1_pnlOnlyGirlChild').style.display = 'block';
        $("#ctl00_ContentPlaceHolder1_ddlOnlyChild").val('0');
    }

}


function onchangeddlBoard() {
    if ($("#ctl00_ContentPlaceHolder1_ddschboard").val() == '99') {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlOtherBoard').style.display = 'block';
        $("#ctl00_ContentPlaceHolder1_txtboard").val('');

    }
    else  {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlOtherBoard').style.display = 'none';
        $("#ctl00_ContentPlaceHolder1_txtboard").val('NA');
    }

}

function onchangeddlDdlImprovement() {
    if ($("#ctl00_ContentPlaceHolder1_ddlImproveMent").val() == '1') {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlImpRollNo').style.display = 'block';
        $("#ctl00_ContentPlaceHolder1_txtImpRollNo").val('');

    }
    else {
        document.getElementById('ctl00_ContentPlaceHolder1_pnlImpRollNo').style.display = 'none';
        $("#ctl00_ContentPlaceHolder1_txtImpRollNo").val('');
    }

}


function ChgXIIPassYearCorr() {

    var ctrlXIIPer = document.getElementById('ctl00_ContentPlaceHolder1_txtperXII');
    var ctrlXIIRoll = document.getElementById('ctl00_ContentPlaceHolder1_txtRollNo');
    var ctrl = document.getElementById('ctl00_ContentPlaceHolder1_ddyearpassXII');
    var ctr2 = document.getElementById('ctl00_ContentPlaceHolder1_ddlYearPassX');
    if (ctrl.value != "0" && ctr2.value != "0") {
        if (parseInt(ctrl.value) - parseInt(ctr2.value) < 2) {
            alert("Minimum difference between 'Year of passing class 10th or its equivalent' and 'Year of passing or appearing class 12th or its equivalent' should be 2 year");
            ctr2.value = "0";
            ctrl.value = "0";
            ctr2.focus();
            return false;
        }
    }

    if (ctrl.value == "2019") {
        ctrlXIIPer.value = "";
        ctrlXIIRoll.value = "";
        ctrlXIIPer.disabled = true;
        ctrlXIIRoll.disabled = true;
        document.getElementById('ctl00_ContentPlaceHolder1_pnlPerc').style.display = 'none';
    }
    else {
        alert(ctrl.value);
        ctrlXIIPer.disabled = false;
        ctrlXIIRoll.disabled = false;
        ctrlXIIPer.value = "";
        ctrlXIIRoll.value = "";
        document.getElementById('ctl00_ContentPlaceHolder1_pnlPerc').style.display = 'block';

    }
    return true;
}

function OnChgCountry() {
    var ctrlCountry = document.getElementById('ctl00_ContentPlaceHolder1_ddlCountry');
    var ctrlState = document.getElementById('ctl00_ContentPlaceHolder1_ddlState');
    var ctrlDistrict = document.getElementById('ctl00_ContentPlaceHolder1_ddlDistrict');
   
    if (ctrlCountry.value != "IN") {
        ctrlDistrict.value = "0";
        ctrlState.value = "0";
//        ctrlState.disabled = true;
//        ctrlDistrict.disabled = true;
        document.getElementById('ctl00_ContentPlaceHolder1_pnlStateDist').style.display = 'none';
    }
    else {

        ctrlState.value = "0";
        ctrlDistrict.value = "0";
//        ctrlState.disabled = false;
//        ctrlDistrict.disabled = false;
        document.getElementById('ctl00_ContentPlaceHolder1_pnlStateDist').style.display = 'block';
    }
    return true;
}

