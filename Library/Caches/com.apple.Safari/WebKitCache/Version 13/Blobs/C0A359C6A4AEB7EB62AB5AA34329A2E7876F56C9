function checkPhotoExtension(sender, args) {

    var flag = false;
    var ctrlf1 = document.getElementById('ctl00_ContentPlaceHolder1_FileUpload1');
    var filePath = ctrlf1.value;

    if (filePath.indexOf('.') == -1) {
        flag = false;
    }

    var validExtensions = new Array();
    var ext = filePath.substring(filePath.lastIndexOf('.') + 1).toLowerCase();

    validExtensions[0] = 'jpg';
    validExtensions[1] = 'jpeg';

    for (var i = 0; i < validExtensions.length; i++) {
        if (ext == validExtensions[i]) {
            flag = true;
        }

    }

    if (flag == false) {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }
}

function checkSignExtension(sender, args) {

    var flag = false;
    var ctrlf1 = document.getElementById('ctl00_ContentPlaceHolder1_FileUpload2');
    var filePath = ctrlf1.value;

    if (filePath.indexOf('.') == -1) {
        flag = false;
    }

    var validExtensions = new Array();
    var ext = filePath.substring(filePath.lastIndexOf('.') + 1).toLowerCase();

    validExtensions[0] = 'jpg';
    validExtensions[1] = 'jpeg';

    for (var i = 0; i < validExtensions.length; i++) {
        if (ext == validExtensions[i]) {
            flag = true;
        }

    }

    if (flag == false) {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }
}




function checkThumbExtension(sender, args) {

    var flag = false;
    var ctrlf1 = document.getElementById('ctl00_ContentPlaceHolder1_FileUpload3');
    var filePath = ctrlf1.value;

    if (filePath.indexOf('.') == -1) {
        flag = false;
    }

    var validExtensions = new Array();
    var ext = filePath.substring(filePath.lastIndexOf('.') + 1).toLowerCase();

    validExtensions[0] = 'jpg';
    validExtensions[1] = 'jpeg';

    for (var i = 0; i < validExtensions.length; i++) {
        if (ext == validExtensions[i]) {
            flag = true;
        }

    }

    if (flag == false) {
        args.IsValid = false;
    }
    else {
        args.IsValid = true;
    }
}
function LogoutConf() {

    var ctrl1 = document.getElementById('ctl00_ContentPlaceHolder1_txtoldpass')
    var ctrl2 = document.getElementById('ctl00_ContentPlaceHolder1_txtnewpass')
    var ctrl3 = document.getElementById('ctl00_ContentPlaceHolder1_txtconfnewpass')
    var ctrl4 = document.getElementById('ctl00_ContentPlaceHolder1_txtsecans1')
    if (ctrl1 != null && ctrl2 != null && ctrl3 != null && ctrl4 != null) {
        ctrl1.value = ""
        ctrl2.value = ""
        ctrl3.value = ""
        ctrl4.value = ""
    }

    var con = confirm('Are you sure, you want to Logout ?')
    if (con == true) {

        return true;

    }

    else {

        return false;

    }
}
function PwDPerc() {

    if (document.getElementById('ctl00_ContentPlaceHolder1_txtPhPer').value != "") {
        var a = 0;
        a = parseInt(document.getElementById('ctl00_ContentPlaceHolder1_txtPhPer').value);
        if (a < 40) {
            document.getElementById('ctl00_ContentPlaceHolder1_ddlneedamanuenses').value = "2";
            document.getElementById('ctl00_ContentPlaceHolder1_ddlneedamanuenses').disabled = true;

        }
        else {
            document.getElementById('ctl00_ContentPlaceHolder1_ddlneedamanuenses').value = "0";
            document.getElementById('ctl00_ContentPlaceHolder1_ddlneedamanuenses').disabled = false;

        }
    }

}
function IsCMFNameValid() {
    var ctrl1 = document.getElementById('ctl00_ContentPlaceHolder1_TxtCname');
    var ctrl2 = document.getElementById('ctl00_ContentPlaceHolder1_Txtmname');
    var ctrl3 = document.getElementById('ctl00_ContentPlaceHolder1_Txtfname');

    if (ctrl1.value != "" && ctrl2.value != "") {
        if (ctrl1.value.toUpperCase() == ctrl2.value.toUpperCase()) {
            alert("Candidate's name & Mother's name should not be same.");
            ctrl2.value = ""
            return;
        }
    }

    if (ctrl1.value != "" && ctrl3.value != "") {
        if (ctrl1.value.toUpperCase() == ctrl3.value.toUpperCase()) {
            alert("Candidate's name & Father's name should not be same.");
            ctrl3.value = ""
            return;
        }
    }

    if (ctrl2.value != "" && ctrl3.value != "") {
        if (ctrl2.value.toUpperCase() == ctrl3.value.toUpperCase()) {
            alert("Mother's name & Father's name should not be same.");
            ctrl3.value = ""
            return;
        }
    }
}

