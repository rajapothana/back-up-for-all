function DoBlur(fld) {
    fld.className = 'textboxes1';
}

function DoFocus(fld) {
    fld.className = 'textboxes3';
}

function ResetDataAppForm() {

    document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value = ""
    document.getElementById('ctl00_ContentPlaceHolder1_txtconfpass').value = ""
    document.getElementById('ctl00_ContentPlaceHolder1_txtSecAns').value = ""
    var con = confirm('Are you sure you want to reset all the Form controls value ?')
    if (con == true) {

        return true;

    }

    else {

        return false;

    }
}


function RefreshCaptcha() {

    document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value = ""
    document.getElementById('ctl00_ContentPlaceHolder1_txtconfpass').value = ""
    document.getElementById('ctl00_ContentPlaceHolder1_txtSecAns').value = ""

}

function isNumeric(n) {
    return !isNaN(parseFloat(n)) && isFinite(n);
}

function onsubclick(salt) {
//    var $val = 0;
//    $("input.inp").each(function () {
//        if (($(this).val()) == "") {
//            $(this).addClass("error");
//            $val = 1
//        }
//        else {
//            $(this).removeClass("error");
//        }

//    });

//    $("select.inp").each(function () {
//        if (($(this).val()) == "0") {
//            $(this).addClass("error1");
//            $(this).css("background-color", "ghostwhite");
//            $(this).next(".chosen-container").addClass("error2");
//            $val = 1
//        }
//        else {
//            $(this).removeClass("error1");
//        }

//    });

//    if ($val > 0) {
//        alert('Please fill the details in hightlighted Required fields.');
//        return false;
//    }
    Page_ClientValidate();
    if (Page_IsValid) {

    }
    else {

        document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value = ""
        document.getElementById('ctl00_ContentPlaceHolder1_txtconfpass').value = ""
        document.getElementById('ctl00_ContentPlaceHolder1_txtSecAns').value = ""
        return (false);
    }


    if ($("#ctl00_ContentPlaceHolder1_ddcodeelig1").val() != 'ZZ') {
        if ($("#ctl00_ContentPlaceHolder1_ddlStateOfSchool").val() != null) {
            if ($("#ctl00_ContentPlaceHolder1_ddcodeelig1").val() != $("#ctl00_ContentPlaceHolder1_ddlStateOfSchool").val() && $("#ctl00_ContentPlaceHolder1_ddlStateOfSchool").val() != 'ZZ') {
                alert("State of eligibility and State of location of your School must be same.For more information please click Help Button.");
                $("#ctl00_ContentPlaceHolder1_ddlStateOfSchool").val('0');
                return (false);
            } 
        }
    }
    if ($("#ctl00_ContentPlaceHolder1_ddcodeelig1").val() != 'ZZ') {
        if ($("#ctl00_ContentPlaceHolder1_ddlPassportState").val() != null) {
            if ($("#ctl00_ContentPlaceHolder1_ddcodeelig1").val() != $("#ctl00_ContentPlaceHolder1_ddlPassportState").val() && $("#ctl00_ContentPlaceHolder1_ddlStateOfSchool").val() != '0' && $("#ctl00_ContentPlaceHolder1_ddlStateOfSchool").val() == 'ZZ') {
                alert("State name mentioned in your Passport and State of eligibility must be same.For more information please click Help Button.");
                $("#ctl00_ContentPlaceHolder1_ddlPassportState").val('0');
                return (false);
            }
        }
    }

 
    var textVerificationId = document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId')
    var textVerificationType = document.getElementById('ctl00_ContentPlaceHolder1_ddlVerificationType')
   if (textVerificationId.value.length > 20 || textVerificationId.value == "") {
       alert('Please Enter Valid Identification Id');
       textVerificationId.value = "";
        return (false);
    }
    if (textVerificationType.value == '01') {
        var flag = false;
        if (isNumeric(textVerificationId.value)) {
            var flag = true;
        }
        else { var flag = false; }

        var len = textVerificationId.value.length
        if (len > 12 || len < 12) {
            alert('Please Enter Valid Aadhaar Number');
            return (false);
        }
        if (flag == false) {
            alert('Please Enter Valid Aadhaar Number');
            return false;
        }

        if (textVerificationId.value == "") {
            alert('Please Enter Identification Id');
            return false;
        }
    }
    var key = CryptoJS.enc.Utf8.parse(salt);
    var iv = CryptoJS.enc.Utf8.parse('8080808080808080');
    var encrypted = CryptoJS.AES.encrypt(CryptoJS.enc.Utf8.parse(textVerificationId.value), key,
                {
                    keySize: 128 / 8,
                    iv: iv,
                    mode: CryptoJS.mode.CBC,
                    padding: CryptoJS.pad.Pkcs7
                });
                document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId').value = encrypted;
  

    if (document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value != "" && document.getElementById('ctl00_ContentPlaceHolder1_txtSecAns').value != "") {
        var passlen = document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value.length;
        if (passlen > 13 || passlen < 8) {
            alert('Password should be 8 to 13 character long.');
            document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId').value = "";
            return (false);
        }

        var a = new RegExp("^(?=.{8,13})(?=.*[A-Z])(?=.*[a-z])(?=.*[0-9])(?=.*\\W).*$", "g");
        if (!a.test(document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value)) {
            alert('Your password must satisfy the following. \n\n* Password should be 8 to 13 character long. \n* Password should have at least one Upper case alphabet.\n* Password should have at least one Lower case alphabet. \n* Password should have at least one numeric value. \n* Password should have at least one special characters eg.!@#$%^&*-');
            document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId').value = "";
            return (false);
        }

        document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value = (SHA256(document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value)).toUpperCase();
        document.getElementById('ctl00_ContentPlaceHolder1_txtconfpass').value = (SHA256(document.getElementById('ctl00_ContentPlaceHolder1_txtconfpass').value)).toUpperCase();
        document.getElementById('ctl00_ContentPlaceHolder1_txtSecAns').value = (SHA256(document.getElementById('ctl00_ContentPlaceHolder1_txtSecAns').value.toUpperCase())).toUpperCase();

        var len = document.getElementById('ctl00_ContentPlaceHolder1_txtpass').value.length;
        if (len > 64 || len < 64) {
            alert('Operation can not be performed.Your browser is not supporting Password Encription.');
            document.getElementById('ctl00_ContentPlaceHolder1_txtVerificationId').value = "";
            return (false);
        }
        return (true);
    }
    else {
        return (true);
    }
}

function DisableBttAfterClick() {
    var btt = document.getElementById('ctl00_ContentPlaceHolder1_btnSubmit');
    Page_ClientValidate();
    if (Page_IsValid) {
        if (btt != null) {
            btt.disabled = true;
        }
    }
}

function DisablePayModeBtt() {
    var btt = document.getElementById('ctl00_ContentPlaceHolder1_btnSubmit');
    btt.disabled = true;

}

function ChgPayMode() {
    var btt = document.getElementById('ctl00_ContentPlaceHolder1_btnSubmit');
    btt.disabled = false;
}


function ChgDeclaration() {
    var ChkIAgree = document.getElementById('ctl00_ContentPlaceHolder1_chkagreement');
    var btnFinalSubmit = document.getElementById('ctl00_ContentPlaceHolder1_btnFinalSubmit');
//    alert($("#ctl00_ContentPlaceHolder1_paramChkList input:checked"));
//    alert($("#ctl00_ContentPlaceHolder1_paramChkList input:checked").length);

    var cnt = $("#ctl00_ContentPlaceHolder1_paramChkList input:checked").length;
//    alert(cnt);
    if (cnt < 10) {
        alert('Kindly check all the check boxes of Parameter Checkbox list, if  all the information are verified and correct.');       
        ChkIAgree.checked = false;
        btnFinalSubmit.disabled = true;
        return;
    }
    if (ChkIAgree.checked == true) {
        btnFinalSubmit.disabled = false;
        $('#ctl00_ContentPlaceHolder1_btnFinalSubmit').removeClass("final_subd");
        $('#ctl00_ContentPlaceHolder1_btnFinalSubmit').addClass("final_sub");
        
        return;
    }
    if (ChkIAgree.checked == false) {
        btnFinalSubmit.disabled = true;
        $('#ctl00_ContentPlaceHolder1_btnFinalSubmit').removeClass("final_sub");
        $('#ctl00_ContentPlaceHolder1_btnFinalSubmit').addClass("final_subd");
        return;
    }
}


function DisableFinalSubmit() {
    var btnFinalSubmit = document.getElementById('ctl00_ContentPlaceHolder1_btnFinalSubmit');
    btnFinalSubmit
    $('#ctl00_ContentPlaceHolder1_btnFinalSubmit').removeClass("final_sub");
    $('#ctl00_ContentPlaceHolder1_btnFinalSubmit').addClass("final_subd");
    btnFinalSubmit.disabled = true;

}

function DisableFinalUpload() {
    var btnFinalSubmit = document.getElementById('ctl00_ContentPlaceHolder1_btnUpload');
    btnFinalSubmit.disabled = true;

}

function ChgFinalUploadDeclaration() {

    var ChkIAgree = document.getElementById('ctl00_ContentPlaceHolder1_chkagreement');
    var btnFinalSubmit = document.getElementById('ctl00_ContentPlaceHolder1_btnUpload');
    if (ChkIAgree.checked == true) {
        btnFinalSubmit.disabled = false;
        return;
    }
    if (ChkIAgree.checked == false) {
        btnFinalSubmit.disabled = true;
        return;
    }
}


