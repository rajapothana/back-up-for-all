function ChgMainExamCentre() {
    var ctrl0 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnQuesPprMedium');
    var ctrl1 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre1');
    var ctrl2 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre2');
    var ctrl3 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre3');
    var ctrl4 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre4');

    if (ctrl1 != null && ctrl2 != null) {
        if (ctrl1.value != "0" && ctrl2.value != "0") {
            if (ctrl1.value == ctrl2.value) {
                alert("The 1st & 2nd Choice of JEE(Main) Exam City should not be same.");
                ctrl2.value = "0"
                return false;
            }
        }
    }

    if (ctrl1 != null && ctrl3 != null) {
        if (ctrl1.value != "0" && ctrl3.value != "0") {
            if (ctrl1.value == ctrl3.value) {
                alert("The 1st & 3rd Choice of JEE(Main) Exam City should not be same.");
                ctrl3.value = "0"
                return false;
            }
        }
    }
    if (ctrl1 != null && ctrl4 != null) {
        if (ctrl1.value != "0" && ctrl4.value != "0") {
            if (ctrl1.value == ctrl4.value) {
                alert("The 1st & 4th Choice of JEE(Main) Exam City should not be same.");
                ctrl4.value = "0"
                return false;
            }
        }
    }
    if (ctrl2 != null && ctrl3 != null) {
        if (ctrl2.value != "0" && ctrl3.value != "0") {
            if (ctrl2.value == ctrl3.value) {
                alert("The 2nd & 3rd Choice of JEE(Main) Exam City should not be same.");
                ctrl3.value = "0"
                return false;
            }
        }
    }
    if (ctrl2 != null && ctrl4 != null) {
        if (ctrl2.value != "0" && ctrl4.value != "0") {
            if (ctrl2.value == ctrl4.value) {
                alert("The 2nd & 4th Choice of JEE(Main) Exam City should not be same.");
                ctrl4.value = "0"
                return false;
            }
        }
    }
    if (ctrl3 != null && ctrl4 != null) {
        if (ctrl3.value != "0" && ctrl4.value != "0") {
            if (ctrl3.value == ctrl4.value) {
                alert("The 3rd & 4th Choice of JEE(Main) Exam City should not be same.");
                ctrl4.value = "0"
                return false;
            }
        }
    }
    if (ctrl1 != null && ctrl2 != null && ctrl3 != null && ctrl4 != null) {
        if (ctrl1.value == "0" || ctrl2.value == "0" || ctrl3.value == "0" || ctrl4.value == "0") {
            return false;
        }
    }

    if (ctrl0.value == "G") {
        ctrl0.value = "0";
    }

    return true;
}

function OnChangeMedium() {
var ctrl0 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnQuesPprMedium');
var ctrl1 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre1').options[document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre1').selectedIndex].text;
var ctrl2 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre2').options[document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre2').selectedIndex].text;
var ctrl3 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre3').options[document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre3').selectedIndex].text;
var ctrl4 = document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre4').options[document.getElementById('ctl00_ContentPlaceHolder1_ddlMnExamCentre4').selectedIndex].text;
       if (ctrl0.value == "G") {
              if (ctrl1.toLowerCase().indexOf("gujarat") == -1 || ctrl2.toLowerCase().indexOf("gujarat") == -1 || ctrl3.toLowerCase().indexOf("gujarat") == -1 || ctrl4.toLowerCase().indexOf("gujarat") == -1) {
                  msg = "Gujarati Question Paper Medium is allowed only for those candidates who opted all the Exam Cities from Gujarat State.";
               alert(msg);
               ctrl0.value = "0";
           }
       }
}

function Chgyear() {

    if (document.getElementById('ctl00_ContentPlaceHolder1_ddyearpassXII').value == "2019") {
        document.getElementById('ctl00_ContentPlaceHolder1_txtperXII').value = "";
        document.getElementById('ctl00_ContentPlaceHolder1_txtperXII').disabled = true;
        document.getElementById('ctl00_ContentPlaceHolder1_txtRollNo').value = "";
        document.getElementById('ctl00_ContentPlaceHolder1_txtRollNo').disabled = true;
    }
    else {
        document.getElementById('ctl00_ContentPlaceHolder1_txtperXII').disabled = false;
        document.getElementById('ctl00_ContentPlaceHolder1_txtRollNo').disabled = false;
    }
    return false;
}


function Chgboard() {
    var ddlReport = document.getElementById('ctl00_ContentPlaceHolder1_ddschboard');
    var ctrlpnlotherboard = document.getElementById('ctl00_ContentPlaceHolder1_pnlOtherBoard');
    var ctrltxtotherboard = document.getElementById('ctl00_ContentPlaceHolder1_txtboard');
    if (ddlReport.value == "99") {
        ctrltxtotherboard.value = ""
        ctrltxtotherboard.disabled = false
        ctrlpnlotherboard.style.visibility = "visible";
    }
    else if (ddlReport.value != "99" || ddlReport.value == "0") {
        ctrltxtotherboard.value = ddlReport.options[ddlReport.selectedIndex].text;
        ctrltxtotherboard.disabled = true
        ctrlpnlotherboard.style.visibility = "hidden";

    }
    return false;
}


