function ChgFinalUploadPhoto() {

    var ChkIAgree = document.getElementById('ctl00_ContentPlaceHolder1_chkagreementP');
    var btnFinalSubmit = document.getElementById('ctl00_ContentPlaceHolder1_btnEditPhoto');
    var btnBrows = document.getElementById('ctl00_ContentPlaceHolder1_FileUpload1');
    if (ChkIAgree.checked == true) {
        btnFinalSubmit.disabled = false;
        btnBrows.disabled = true;
        return;
    }
    if (ChkIAgree.checked == false) {
        btnFinalSubmit.disabled = true;
        btnBrows.disabled = false;
        return;
    }
}
